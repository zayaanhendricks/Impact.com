{
  "testCases": [
    { "description": "Single value", 
      "input": "7", 
      "expected": "7" },
    { "description": "Two ascending", 
      "input": "12,13", 
      "expected": "12-13" },
    { "description": "Two descending", 
      "input": "5,4", 
      "expected": "5-4" },
    { "description": "All ascending", 
      "input": "1,2,3", 
      "expected": "1-3" },
    { "description": "All descending", 
      "input": "3,2,1", 
      "expected": "3-1" },
    { "description": "Up then jump then up",
      "input": "1,2,4,5", 
      "expected": "1-2, 4-5" },
    { "description": "Mix with negatives", 
      "input": "-2,-1,0,1,3",
      "expected": "-2-1, 3" },
    { "description": "Down then up", 
      "input": "6,5,4,5,6,7", 
      "expected": "6-4, 5-7" },
    { "description": "Duplicates break", 
      "input": "2,2,3,4", 
      "expected": "2, 2-4" },
    { "description": "Zeros and ones", 
      "input": "0,1,0,-1", 
      "expected": "0-1, 0--1" },
    { "description": "Jump of 2 breaks", 
      "input": "10,8,6,7,8", 
      "expected": "10, 8, 6-8" },
    { "description": "Long run then singleton", 
      "input": "1,2,3,4,10", 
      "expected": "1-4, 10" }
  ]
}
